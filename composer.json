{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "~7.1.0",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "doctrine/migrations": "~1.4",
        "doctrine/orm": "~2.5",
        "jms/serializer-bundle": "~2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "^4.0",
        "symfony/symfony": "~3.4",
        "twig/twig": "^2.4",
        "symfony/assetic-bundle": "~2.8"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {

        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "env-populate": {
            "files": [
                {
                    "example-file": "config/packages/dev/.env.dist",
                    "generated-file": "config/packages/dev/.env"
                },
                {
                    "example-file": "config/packages/prod/.env.dist",
                    "generated-file": "config/packages/prod/.env"
                },
                {
                    "example-file": "config/packages/test/.env.dist",
                    "generated-file": "config/packages/test/.env"
                }
            ]
        }
    }
}
